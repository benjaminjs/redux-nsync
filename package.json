{
  "name": "redux-nsync",
  "version": "1.0.1",
  "description": "State sync for Redux apps",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage",
    "debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjaminjs/redux-nsync.git"
  },
  "author": "Benjamin Armstrong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benjaminjs/redux-nsync/issues"
  },
  "homepage": "https://github.com/benjaminjs/redux-nsync#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "@types/redux-mock-store": "0.0.13",
    "jest": "^22.4.2",
    "redux": "^3.7.2",
    "redux-mock-store": "1.4.0",
    "ts-jest": "^22.4.2",
    "typescript": "^2.7.2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/test/*.+(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
